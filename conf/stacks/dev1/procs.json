{
  "description": "US Equities DEV",
  "vars": {
    "qhome": "/opt/q",
    "arch": "m64",
    "qbin": "/opt/q/m64/q",
    "c2_port": "${env:QT_C2_PORT}",
    "data": "${env:QT_DATA_DEV1}",
    "cpu_affinity": ""
  },
  "processes": {
    "tp0": {
      "proc": "tp",
      "port_offset": 10,
      "args": [
        "-t",
        "100"
      ]
    },
    "rdb": {
      "proc": "rdb",
      "port_offset": 11,
      "subscribe": {
        "ctp": []
      }
    },
    "wdb": {
      "proc": "wdb",
      "port_offset": 12,
      "dir" : "${data}/wdb",
      "subscribe": {
        "ctp": []
      },
      "args": [
        "-write_every",
        "00:30",
        "-hdb",
        "hdb"
      ]
    },
    "tp1": {
      "proc": "tp",
      "port_offset": 21,
      "args": [
        "-t",
        "10"
      ]
    },
    "ctp": {
      "proc": "ctp",
      "port_offset": 13,
      "subscribe": {
        "tp0": [],
        "tp1": []
      }
    },
    "hdb": {
      "proc": "hdb",
      "port_offset": 20,
      "dir" : "${data}/hdb"
    },
    "eng1": {
      "proc": "eng",
      "port_offset": 30,
      "depends_on" : ["tp0", "net"]
    },
    "net": {
      "proc": "net",
      "port_offset": 40,
      "depends_on" : ["tp0", "om"]
    },
    "om": {
      "proc": "om",
      "port_offset": 50,
      "depends_on" : ["tp0", "devbroker"]
    },
    "devbroker": {
      "proc": "devbroker",
      "port_offset": 51,
      "depends_on" : ["tp0"]
    },
    "c2": {
      "proc": "c2",
      "port_offset": 0
    }
  }
}